@prefix : <http://standards.iso.org/iso/15926/part14#> .
@prefix dc: <http://purl.org/dc/elements/1.1/> .
@prefix lis: <http://standards.iso.org/iso/15926/part14/> .
@prefix obo: <http://purl.obolibrary.org/obo/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix pav: <http://purl.org/pav/> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix lis2: <http://rds.posccaesar.org/2008/02/OWL/ISO-15926-2_2003#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix skos: <http://www.w3.org/2004/02/skos/core#> .
@prefix lis12: <http://standards.iso.org/iso/15926/ontology/life-cycle-integration/> .
@base <http://standards.iso.org/iso/15926/part14> .

<http://standards.iso.org/iso/15926/part14> rdf:type owl:Ontology ;
                                             owl:versionIRI <http://standards.iso.org/iso/15926/part14/1.1> ;
                                             pav:previousVersion <http://standards.iso.org/iso/15926/part14/1.0> ;
                                             rdfs:comment "The ISO 15926-14 upper ontology is an OWL 2 DL rendering of the ISO 15926-2 data model." ;
                                             rdfs:label "ISO 15926-14 upper ontology" ;
                                             owl:versionInfo "Revised 2020-09-29" .

#################################################################
#    Annotation properties
#################################################################

###  http://purl.org/pav/previousVersion
pav:previousVersion rdf:type owl:AnnotationProperty ;
                    rdfs:isDefinedBy pav: ;
                    rdfs:label "previousVersion" .


###  http://standards.iso.org/iso/15926/part14/deprecatedEntity
lis:deprecatedEntity rdf:type owl:AnnotationProperty ;
                     rdfs:comment "Annotation property for referring from OWL ontology resources to semantically similar entities in external vocabularies, where the limitations of OWL, as as first-order language, imply that representing the intended meaning of the referenced entity is not feasible." ;
                     rdfs:label "deprecatedEntity" ;
                     rdfs:subPropertyOf lis:relatedEntity .


###  http://standards.iso.org/iso/15926/part14/deprecatedEntityISO15926
lis:deprecatedEntityISO15926 rdf:type owl:AnnotationProperty ;
                             rdfs:comment "Annotation property for referring from ontology resources in ISO 15926-14 to semantically similar entities in other parts of ISO 15926 (primarily Part 2), where the limitations of OWL, as as first-order language, imply that Part 14 is not capable of expressing the intended meaning of the referenced entity." ;
                             rdfs:label "deprecatedEntityISO15926" ;
                             skos:example "Modal notions can not be captured in OWL. Therefore, Part 2 entities that are essentially modal, such as those referring to \"actuality\" or \"materialization\", need to be left out of Part 14." ;
                             rdfs:subPropertyOf lis:deprecatedEntity ,
                                                lis:relatedEntityISO15926 .


###  http://standards.iso.org/iso/15926/part14/equivalentEntity
lis:equivalentEntity rdf:type owl:AnnotationProperty ;
                     rdfs:comment "Annotation property for referring from OWL ontology resources to entities in external vocabularies, where the local resource is equivalent in meaning to that of the referenced entity." ;
                     rdfs:label "equivalentEntity" ;
                     rdfs:subPropertyOf lis:relatedEntity .


###  http://standards.iso.org/iso/15926/part14/equivalentEntityISO15926
lis:equivalentEntityISO15926 rdf:type owl:AnnotationProperty ;
                             rdfs:comment "Annotation property for referring from ontology resources in ISO 15926-14 to entities in other parts of ISO 15926, where the Part 14 resource is equivalent in meaning to that of the referenced entity." ;
                             rdfs:label "equivalentEntityISO15926" ;
                             skos:example "The Part 14 class Activity has, intuitively, the same meaning as the Part 2 entity Activity, and this annotation property should be used to record that fact." ;
                             rdfs:subPropertyOf lis:equivalentEntity ,
                                                lis:relatedEntityISO15926 .


###  http://standards.iso.org/iso/15926/part14/mergedFrom
lis:mergedFrom rdf:type owl:AnnotationProperty ;
               rdfs:comment "This annotation property is used for stating that the current resource originates from one or several other resources by merging." ;
               rdfs:label "mergedFrom" ;
               skos:prefLabel "mergedFrom" ;
               rdfs:subPropertyOf lis:originatesFrom .


###  http://standards.iso.org/iso/15926/part14/originatesFrom
lis:originatesFrom rdf:type owl:AnnotationProperty ;
                   rdfs:comment "This is a super-property for associating a resource with a resource it originated from." ;
                   rdfs:label "originatesFrom" ;
                   skos:prefLabel "originatesFrom" .


###  http://standards.iso.org/iso/15926/part14/relatedEntity
lis:relatedEntity rdf:type owl:AnnotationProperty ;
                  rdfs:comment "Annotation property for referring from OWL ontology resources to entities in external vocabularies (typically, but not necessarily, ontologies), where definitions, examples, or explications of the external entity contribute to understanding and defining the local resource." ;
                  rdfs:label "relatedEntity" .


###  http://standards.iso.org/iso/15926/part14/relatedEntityISO15926
lis:relatedEntityISO15926 rdf:type owl:AnnotationProperty ;
                          rdfs:comment "Annotation property for referring from OWL ontology resources in ISO 15926-14 to entities in other parts of ISO 15926, where definitions, examples, or explications of the Part 2 entity contribute to understanding and defining the Part 14 resource." ;
                          rdfs:label "relatedEntityISO15926" ;
                          skos:example "The Part 14 class Activity is clearly related to, but not equivalent to, the Part 2 entity ClassOfActivity, and this annotation property should be used to record that fact." ;
                          rdfs:subPropertyOf lis:relatedEntity .


###  http://standards.iso.org/iso/15926/part14/remodelsEntity
lis:remodelsEntity rdf:type owl:AnnotationProperty ;
                   rdfs:comment "Annotation property for referring from OWL ontology resources to entities in external vocabularies, where the local resource, together with OWL constructs, can express intuitively the same facts as the referenced entity." ;
                   rdfs:label "remodelsEntity" ;
                   rdfs:subPropertyOf lis:relatedEntity .


###  http://standards.iso.org/iso/15926/part14/remodelsEntityISO15926
lis:remodelsEntityISO15926 rdf:type owl:AnnotationProperty ;
                           rdfs:comment "Annotation property for referring from OWL ontology resources in ISO 15926-14 to entities in other parts of ISO 15926 (primarily Part 2), where the Part 14 entity type, together with OWL constructs, can express intuitively the same facts as the referenced entity." ;
                           rdfs:label "remodelsEntityISO15926" ;
                           skos:example "Range restrictions on physical quantities can be captured with OWL and Part 14 using \"facet\" datatype restrictions. These will correspond to Part 2 Property Range expressions." ;
                           rdfs:subPropertyOf lis:relatedEntityISO15926 ,
                                              lis:remodelsEntity .


###  http://standards.iso.org/iso/15926/part14/splitFrom
lis:splitFrom rdf:type owl:AnnotationProperty ;
              rdfs:comment "This annotation property is used for stating that the current resource originates from another resource by splitting." ;
              rdfs:label "splitFrom" ;
              skos:prefLabel "splitFrom" ;
              rdfs:subPropertyOf lis:originatesFrom .


###  http://standards.iso.org/iso/15926/part14/transformedFrom
lis:transformedFrom rdf:type owl:AnnotationProperty ;
                    rdfs:comment "This annotation property is used for stating that the current resource originates from another resource by transformation." ;
                    rdfs:label "transformedFrom" ;
                    skos:prefLabel "transformedFrom" ;
                    rdfs:subPropertyOf lis:originatesFrom .


###  http://www.w3.org/2000/01/rdf-schema#comment
rdfs:comment rdf:type owl:AnnotationProperty .


###  http://www.w3.org/2000/01/rdf-schema#isDefinedBy
rdfs:isDefinedBy rdf:type owl:AnnotationProperty .


###  http://www.w3.org/2000/01/rdf-schema#label
rdfs:label rdf:type owl:AnnotationProperty .


###  http://www.w3.org/2000/01/rdf-schema#seeAlso
rdfs:seeAlso rdf:type owl:AnnotationProperty .


###  http://www.w3.org/2002/07/owl#versionInfo
owl:versionInfo rdf:type owl:AnnotationProperty .


###  http://www.w3.org/2004/02/skos/core#altLabel
skos:altLabel rdf:type owl:AnnotationProperty ;
              rdfs:comment "Alternative label for the ontology resource." ;
              rdfs:isDefinedBy "http://www.w3.org/2004/02/skos/core" ;
              rdfs:label "altLabel" .


###  http://www.w3.org/2004/02/skos/core#definition
skos:definition rdf:type owl:AnnotationProperty ;
                rdfs:comment "This SKOS relation replaces the Part 2 \"definition\" attribute." ;
                rdfs:isDefinedBy "http://www.w3.org/2004/02/skos/core" ;
                rdfs:label "definition" .


###  http://www.w3.org/2004/02/skos/core#example
skos:example rdf:type owl:AnnotationProperty ;
             rdfs:comment "This SKOS relation replaces the Part 2 \"example\" attribute." ;
             rdfs:isDefinedBy "http://www.w3.org/2004/02/skos/core" ;
             rdfs:label "example" .


###  http://www.w3.org/2004/02/skos/core#note
skos:note rdf:type owl:AnnotationProperty ;
          rdfs:comment "This SKOS relation replaces the Part 2 \"description\" attribute." ;
          rdfs:isDefinedBy "http://www.w3.org/2004/02/skos/core" ;
          rdfs:label "note" .


###  http://www.w3.org/2004/02/skos/core#prefLabel
skos:prefLabel rdf:type owl:AnnotationProperty ;
               rdfs:comment "Preferred label for the ontology resource." ;
               rdfs:isDefinedBy "http://www.w3.org/2004/02/skos/core" ;
               rdfs:label "prefLabel" .


#################################################################
#    Datatypes
#################################################################

###  http://www.w3.org/1999/02/22-rdf-syntax-ns#PlainLiteral
rdf:PlainLiteral rdf:type rdfs:Datatype .


###  http://www.w3.org/2000/01/rdf-schema#Literal
rdfs:Literal rdf:type rdfs:Datatype .


###  http://www.w3.org/2001/XMLSchema#dateTime
xsd:dateTime rdf:type rdfs:Datatype .


###  http://www.w3.org/2001/XMLSchema#decimal
xsd:decimal rdf:type rdfs:Datatype .


###  http://www.w3.org/2001/XMLSchema#integer
xsd:integer rdf:type rdfs:Datatype .


###  http://www.w3.org/2001/XMLSchema#string
xsd:string rdf:type rdfs:Datatype .


#################################################################
#    Object Properties
#################################################################

###  http://standards.iso.org/iso/15926/part14/activityBoundOf
lis:activityBoundOf rdf:type owl:ObjectProperty ;
                    rdfs:subPropertyOf lis:activityPartOf ;
                    owl:inverseOf lis:hasActivityBound ;
                    rdfs:label "activityBoundOf" ;
                    skos:prefLabel "activityBoundOf" .


###  http://standards.iso.org/iso/15926/part14/activityPartOf
lis:activityPartOf rdf:type owl:ObjectProperty ;
                   rdfs:subPropertyOf lis:partOf ;
                   owl:inverseOf lis:hasActivityPart ;
                   rdfs:label "activityPartOf" ;
                   skos:prefLabel "activityPartOf" .


###  http://standards.iso.org/iso/15926/part14/after
lis:after rdf:type owl:ObjectProperty ;
          rdfs:subPropertyOf lis:occursRelativeTo ;
          owl:inverseOf lis:before ;
          rdfs:comment "Use this relation to state that one activity after before another." ;
          rdfs:label "after" ;
          skos:prefLabel "after" .


###  http://standards.iso.org/iso/15926/part14/approvedBy
lis:approvedBy rdf:type owl:ObjectProperty ;
               rdfs:subPropertyOf lis:interestOf ;
               owl:inverseOf lis:approves ;
               lis:equivalentEntityISO15926 lis2:Approval ;
               rdfs:comment "Relation for stating that some item or activity was approved by an entity, typically a person or an organisation." ;
               rdfs:label "approvedBy" ;
               skos:prefLabel "approvedBy" .


###  http://standards.iso.org/iso/15926/part14/approves
lis:approves rdf:type owl:ObjectProperty ;
             rdfs:subPropertyOf lis:hasInterestIn ;
             lis:equivalentEntityISO15926 lis2:Approval ;
             rdfs:label "approves" ;
             skos:prefLabel "approves" .


###  http://standards.iso.org/iso/15926/part14/arrangedPartOf
lis:arrangedPartOf rdf:type owl:ObjectProperty ;
                   rdfs:subPropertyOf lis:partOf ;
                   owl:inverseOf lis:hasArrangedPart ;
                   lis:equivalentEntityISO15926 lis2:ArrangementOfIndividual ,
                                                lis12:arrangedPartOf ;
                   rdfs:label "arrangedPartOf" ;
                   skos:prefLabel "arrangedPartOf" .


###  http://standards.iso.org/iso/15926/part14/assembledPartOf
lis:assembledPartOf rdf:type owl:ObjectProperty ;
                    rdfs:subPropertyOf lis:arrangedPartOf ;
                    owl:inverseOf lis:hasAssembledPart ;
                    lis:equivalentEntityISO15926 lis2:AssemblyOfIndividual ,
                                                 lis12:assembledPartOf ;
                    rdfs:label "assembledPartOf" ;
                    skos:prefLabel "assembledPartOf" .


###  http://standards.iso.org/iso/15926/part14/before
lis:before rdf:type owl:ObjectProperty ;
           rdfs:subPropertyOf lis:occursRelativeTo ;
           rdfs:comment "Use this relation to state that one activity occurs before another." ;
           rdfs:label "before" ;
           skos:prefLabel "before" .


###  http://standards.iso.org/iso/15926/part14/begins
lis:begins rdf:type owl:ObjectProperty ;
           rdfs:subPropertyOf lis:activityBoundOf ;
           owl:inverseOf lis:hasBeginning ;
           lis:equivalentEntityISO15926 lis2:Beginning ,
                                        lis12:begins ;
           rdfs:label "begins" ;
           skos:prefLabel "begins" .


###  http://standards.iso.org/iso/15926/part14/causedBy
lis:causedBy rdf:type owl:ObjectProperty ;
             rdfs:subPropertyOf lis:after ;
             owl:inverseOf lis:causes ;
             lis:equivalentEntityISO15926 lis2:CauseOfEvent ;
             lis:relatedEntityISO15926 lis12:causes ;
             rdfs:label "causedBy" ;
             skos:prefLabel "causedBy" .


###  http://standards.iso.org/iso/15926/part14/causes
lis:causes rdf:type owl:ObjectProperty ;
           rdfs:subPropertyOf lis:before ;
           lis:equivalentEntityISO15926 lis2:CauseOfEvent ,
                                        lis12:causes ;
           rdfs:label "causes" ;
           skos:prefLabel "causes" .


###  http://standards.iso.org/iso/15926/part14/concretizedBy
lis:concretizedBy rdf:type owl:ObjectProperty ;
                  owl:inverseOf lis:concretizes ;
                  lis:relatedEntityISO15926 lis12:InformationObject ;
                  rdfs:label "concretizedBy" ;
                  skos:prefLabel "concretizedBy" .


###  http://standards.iso.org/iso/15926/part14/concretizes
lis:concretizes rdf:type owl:ObjectProperty ;
                rdfs:domain [ rdf:type owl:Class ;
                              owl:unionOf ( lis:Activity
                                            lis:Feature
                                          )
                            ] ;
                rdfs:range lis:InformationObject ;
                lis:relatedEntityISO15926 lis12:InformationObject ;
                rdfs:comment "Inspired by BFO's \"concretizes\". Note that the ISO 15926-14 definition diverges slightly from that in BFO, mainly in employing Feature where BFO has \"specifically dependent continuant\"." ,
                             "TODO. Consider splitting this into two relations, one concretizesInFeature and another concretizesInActivity, to avoid a disjunctive domain." ;
                rdfs:label "concretizes" ;
                rdfs:seeAlso obo:BFO_0000164 ,
                             obo:RO_0000059 ;
                skos:prefLabel "concretizes" .


###  http://standards.iso.org/iso/15926/part14/connectedTo
lis:connectedTo rdf:type owl:ObjectProperty ,
                         owl:SymmetricProperty ;
                rdfs:domain lis:PhysicalObject ;
                lis:equivalentEntityISO15926 lis2:ConnectionOfIndividual ,
                                             lis12:connectedTo ;
                rdfs:label "connectedTo" ;
                skos:prefLabel "connectedTo" .


###  http://standards.iso.org/iso/15926/part14/containedBy
lis:containedBy rdf:type owl:ObjectProperty ;
                rdfs:subPropertyOf lis:locatedRelativeTo ;
                owl:inverseOf lis:contains ;
                lis:equivalentEntityISO15926 lis2:ContainmentOfIndividual ,
                                             lis12:containedBy ;
                rdfs:label "containedBy" ;
                skos:prefLabel "containedBy" .


###  http://standards.iso.org/iso/15926/part14/contains
lis:contains rdf:type owl:ObjectProperty ;
             rdfs:subPropertyOf lis:locatedRelativeTo ;
             rdfs:domain lis:PhysicalObject ;
             rdfs:range lis:PhysicalObject ;
             lis:equivalentEntityISO15926 lis2:ContainmentOfIndividual ,
                                          lis12:contains ;
             rdfs:comment "For the DL profile, we restrict this relation to physical objects. Note that this rules out using \"lis:contains\" for spatial locations." ;
             rdfs:label "contains" ;
             skos:prefLabel "contains" .


###  http://standards.iso.org/iso/15926/part14/createdBy
lis:createdBy rdf:type owl:ObjectProperty ;
              owl:inverseOf lis:creates ;
              lis:equivalentEntityISO15926 lis12:hasCauseOfBeginning ;
              lis:relatedEntityISO15926 lis2:ClassOfCauseOfBeginningOfClassOfIndividual ;
              rdfs:label "createdBy" ;
              skos:prefLabel "createdBy" .


###  http://standards.iso.org/iso/15926/part14/creates
lis:creates rdf:type owl:ObjectProperty ;
            rdfs:domain lis:Activity ;
            rdfs:range lis:PhysicalObject ;
            lis:equivalentEntityISO15926 lis12:causesBeginningOf ;
            lis:relatedEntityISO15926 lis2:ClassOfCauseOfBeginningOfClassOfIndividual ;
            rdfs:comment "Use this relation to express that an activity brings a physical object into being. (Derived from class_of_cause_of_beginning_of_class_of_individual in Part 2)." ;
            rdfs:label "creates" ;
            skos:prefLabel "creates" .


###  http://standards.iso.org/iso/15926/part14/datumUOM
lis:datumUOM rdf:type owl:ObjectProperty ,
                      owl:FunctionalProperty ;
             rdfs:domain lis:QuantityDatum ;
             rdfs:range lis:UnitOfMeasure ;
             rdfs:comment "Relation (functional) to assign unit of measure to measurement data. There is no obvious analogue in ISO 15926-2 or ISO 15926-12." ;
             rdfs:label "datumUOM" ;
             skos:prefLabel "datumUOM" .


###  http://standards.iso.org/iso/15926/part14/directlyConnectedTo
lis:directlyConnectedTo rdf:type owl:ObjectProperty ;
                        rdfs:subPropertyOf lis:connectedTo ;
                        rdf:type owl:SymmetricProperty ;
                        lis:equivalentEntityISO15926 lis2:DirectConnection ,
                                                     lis12:directlyConnectedTo ;
                        rdfs:label "directlyConnectedTo" ;
                        skos:prefLabel "directlyConnectedTo" .


###  http://standards.iso.org/iso/15926/part14/dispositionOf
lis:dispositionOf rdf:type owl:ObjectProperty ;
                  owl:inverseOf lis:hasDisposition ;
                  rdfs:label "dispositionOf" ;
                  skos:prefLabel "dispositionOf" .


###  http://standards.iso.org/iso/15926/part14/ends
lis:ends rdf:type owl:ObjectProperty ;
         rdfs:subPropertyOf lis:activityBoundOf ;
         owl:inverseOf lis:hasEnding ;
         rdfs:label "ends" ;
         skos:prefLabel "ends" .


###  http://standards.iso.org/iso/15926/part14/featureOf
lis:featureOf rdf:type owl:ObjectProperty ;
              rdfs:subPropertyOf lis:arrangedPartOf ;
              owl:inverseOf lis:hasFeature ;
              lis:equivalentEntityISO15926 lis2:FeatureWholePart ,
                                           lis12:featureOf ;
              rdfs:label "featureOf" ;
              skos:prefLabel "featureOf" .


###  http://standards.iso.org/iso/15926/part14/functionOf
lis:functionOf rdf:type owl:ObjectProperty ;
               rdfs:subPropertyOf lis:dispositionOf ;
               owl:inverseOf lis:hasFunction ;
               rdfs:label "functionOf" ;
               skos:prefLabel "functionOf" .


###  http://standards.iso.org/iso/15926/part14/functionalPartOf
lis:functionalPartOf rdf:type owl:ObjectProperty ;
                     rdfs:subPropertyOf lis:partOf ;
                     owl:inverseOf lis:hasFunctionalPart ;
                     rdfs:domain lis:FunctionalObject ;
                     rdfs:range lis:System ;
                     rdfs:comment "Used to capture the functional break-down of a system. Where x is a functional part of a system y, the realisation of one or more functions of x contributes to the performance of y." ;
                     rdfs:label "functionalPartOf" ;
                     skos:prefLabel "functionalPartOf" .


###  http://standards.iso.org/iso/15926/part14/hasActivityBound
lis:hasActivityBound rdf:type owl:ObjectProperty ;
                     rdfs:subPropertyOf lis:hasActivityPart ;
                     lis:equivalentEntityISO15926 lis2:TemporalBounding ,
                                                  lis12:hasTemporalBound ;
                     rdfs:label "hasActivityBound" ;
                     skos:prefLabel "hasActivityBound" .


###  http://standards.iso.org/iso/15926/part14/hasActivityPart
lis:hasActivityPart rdf:type owl:ObjectProperty ;
                    rdfs:subPropertyOf lis:hasPart ;
                    rdfs:domain lis:Activity ;
                    rdfs:range lis:Activity ;
                    lis:relatedEntityISO15926 lis2:TemporalWholePart ,
                                              lis12:hasTemporalPart ;
                    rdfs:label "hasActivityPart" ;
                    skos:prefLabel "hasActivityPart" .


###  http://standards.iso.org/iso/15926/part14/hasArrangedPart
lis:hasArrangedPart rdf:type owl:ObjectProperty ;
                    rdfs:subPropertyOf lis:hasPart ;
                    rdfs:domain lis:PhysicalObject ;
                    lis:equivalentEntityISO15926 lis2:ArrangementOfIndividual ,
                                                 lis12:hasArrangedPart ;
                    rdfs:comment "In line with intended use, for the DL profile this relation has a domain restricted to physical objects." ;
                    rdfs:label "hasArrangedPart" ;
                    skos:prefLabel "hasArrangedPart" .


###  http://standards.iso.org/iso/15926/part14/hasAssembledPart
lis:hasAssembledPart rdf:type owl:ObjectProperty ;
                     rdfs:subPropertyOf lis:hasArrangedPart ;
                     lis:equivalentEntityISO15926 lis2:AssemblyOfIndividual ,
                                                  lis12:hasAssembledPart ;
                     rdfs:comment "This is the recommended (super-) relation for capturing physical breakdown of mechanical assemblies." ;
                     rdfs:label "hasAssembledPart" ;
                     skos:prefLabel "hasAssembledPart" .


###  http://standards.iso.org/iso/15926/part14/hasBeginning
lis:hasBeginning rdf:type owl:ObjectProperty ;
                 rdfs:subPropertyOf lis:hasActivityBound ;
                 lis:equivalentEntityISO15926 lis2:Beginning ,
                                              lis12:hasBeginning ;
                 rdfs:label "hasBeginning" ;
                 skos:prefLabel "hasBeginning" .


###  http://standards.iso.org/iso/15926/part14/hasDisposition
lis:hasDisposition rdf:type owl:ObjectProperty ;
                   rdfs:range lis:Disposition ;
                   rdfs:comment "Inspired by BFO's \"has disposition\" (RO_0000091)." ;
                   rdfs:label "hasDisposition" ;
                   rdfs:seeAlso obo:RO_0000091 ;
                   skos:prefLabel "hasDisposition" .


###  http://standards.iso.org/iso/15926/part14/hasEnding
lis:hasEnding rdf:type owl:ObjectProperty ;
              rdfs:subPropertyOf lis:hasActivityBound ;
              lis:equivalentEntityISO15926 lis2:Ending ,
                                           lis12:hasEnd ;
              rdfs:label "hasEnding" ;
              skos:prefLabel "hasEnding" .


###  http://standards.iso.org/iso/15926/part14/hasFeature
lis:hasFeature rdf:type owl:ObjectProperty ;
               rdfs:subPropertyOf lis:hasArrangedPart ;
               lis:equivalentEntityISO15926 lis2:FeatureWholePart ,
                                            lis12:hasFeature ;
               rdfs:comment "Example of usage: stating that an entity has a surface suitable for connection, such as a flange face." ;
               rdfs:label "hasFeature" ;
               skos:prefLabel "hasFeature" .


###  http://standards.iso.org/iso/15926/part14/hasFunction
lis:hasFunction rdf:type owl:ObjectProperty ;
                rdfs:subPropertyOf lis:hasDisposition ;
                rdfs:range lis:Function ;
                lis:relatedEntityISO15926 lis2:FunctionalPhysicalObject ,
                                          lis12:FunctionalPhysicalObject ;
                rdfs:comment "Inspired by BFO's \"has function\" (RO_0000085)." ;
                rdfs:label "hasFunction" ;
                rdfs:seeAlso obo:RO_0000085 ;
                skos:prefLabel "hasFunction" .


###  http://standards.iso.org/iso/15926/part14/hasFunctionalPart
lis:hasFunctionalPart rdf:type owl:ObjectProperty ;
                      rdfs:subPropertyOf lis:hasPart ;
                      rdfs:label "hasFunctionalPart" ;
                      skos:prefLabel "hasFunctionalPart" .


###  http://standards.iso.org/iso/15926/part14/hasInstalled
lis:hasInstalled rdf:type owl:ObjectProperty ;
                 owl:inverseOf lis:installedAs ;
                 rdfs:label "hasInstalled" ;
                 skos:prefLabel "hasInstalled" .


###  http://standards.iso.org/iso/15926/part14/hasInterestIn
lis:hasInterestIn rdf:type owl:ObjectProperty ;
                  owl:inverseOf lis:interestOf ;
                  lis:relatedEntityISO15926 lis2:LifecycleStage ;
                  rdfs:label "hasInterestIn" ;
                  skos:prefLabel "hasInterestIn" .


###  http://standards.iso.org/iso/15926/part14/hasPart
lis:hasPart rdf:type owl:ObjectProperty ;
            owl:inverseOf lis:partOf ;
            lis:equivalentEntityISO15926 lis12:hasPart ,
                                         "CompositionOfIndividual" ;
            rdfs:label "hasPart" ;
            skos:prefLabel "hasPart" .


###  http://standards.iso.org/iso/15926/part14/hasParticipant
lis:hasParticipant rdf:type owl:ObjectProperty ;
                   owl:inverseOf lis:participantIn ;
                   rdfs:domain lis:Activity ;
                   lis:equivalentEntityISO15926 lis2:Participation ,
                                                lis12:hasParticipant ;
                   rdfs:comment "Note that BFO does not have 'has participant' as a subrelation of 'has part'. This can be motivated in that 4D objects, here members of Activity, are at least not obviously able to have 3D parts." ,
                                "This is the recommended superrelation for types of participation by entities in activities -- the agent, the matter being acted upon, etc." ;
                   rdfs:label "hasParticipant" ;
                   skos:prefLabel "hasParticipant" .


###  http://standards.iso.org/iso/15926/part14/hasPhysicalQuantity
lis:hasPhysicalQuantity rdf:type owl:ObjectProperty ;
                        rdfs:subPropertyOf lis:hasQuality ;
                        owl:inverseOf lis:physicalQuantityOf ;
